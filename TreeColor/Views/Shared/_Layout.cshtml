<html>
    <head>
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <link type="text/css" rel="stylesheet" href="~/Content/materialize.min.css" media="screen,projection" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <script src="~/Scripts/jquery-3.2.1.js"></script>
    </head>
    <body>
        <ul id="dropdown1" class="dropdown-content">
            <li><a href="#!"><i class="material-icons left">settings</i>Settings</a></li>
            <li class="divider"></li>
            <li><a href="#!"><i class="material-icons left">exit_to_app</i>Exit</a></li>
        </ul>
        <ul id="dropdown2" class="dropdown-content">
            @using (Html.BeginForm("ChangeTest", "Home"))
            {
                if (ViewBag.Tests != null)
                {
                    foreach (TreeColor.Models.Tests test in ViewBag.Tests)
                    {
                        <li><a href=@Url.Action("ChangeTest", "Home", new { id = test.id})>@test.test_name</a></li>
                    }
                }
            }
        </ul>
        <nav>
            <div class="nav-wrapper">
                <a href="#!" class="brand-logo">Start test</a>
                <ul class="right hide-on-med-and-down">
                   
                    <li><a href="sass.html">@ViewBag.TestName</a></li>
                    <li><a class="dropdown-button" href="#!" data-activates="dropdown1">Account <i class="material-icons right">arrow_drop_down</i></a></li>
                    <li><a class="dropdown-button" href="#!" data-activates="dropdown2">Choose the Test <i class="material-icons right">arrow_drop_down</i></a></li>
                </ul>
</div>

        </nav>
            @RenderBody()
        <script type="text/javascript" src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
        <script src="~/Scripts/materialize.min.js"></script>
    </body>
</html>
